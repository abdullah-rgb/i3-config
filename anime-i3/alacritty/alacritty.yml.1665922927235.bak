colors:
  name: Jason Wryan
  author: ""
  primary:
    background: "#000000"
    foreground: "#ffffff"
  cursor:
    text: "#000000"
    cursor: "#ffffff"
  normal:
    black: "#222222"
    red: "#9e5641"
    green: "#6c7e55"
    yellow: "#caaf2b"
    blue: "#4c8ea1"
    magenta: "#956d9d"
    cyan: "#7c9aa6"
    white: "#909090"
  bright:
    black: "#454545"
    red: "#cc896d"
    green: "#7db37d"
    yellow: "#bfb556"
    blue: "#6bc1d0"
    magenta: "#c18fcb"
    cyan: "#8fadbf"
