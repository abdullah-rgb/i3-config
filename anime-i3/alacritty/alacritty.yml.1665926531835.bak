colors:
  name: Gnometerm
  author: ""
  primary:
    background: "#000000"
    foreground: "#ffffff"
  cursor:
    text: "#000000"
    cursor: "#ffffff"
  normal:
    black: "#000000"
    red: "#cc0000"
    green: "#4e9a06"
    yellow: "#c4a000"
    blue: "#3465a4"
    magenta: "#75507b"
    cyan: "#06989a"
    white: "#d3d7cf"
  bright:
    black: "#555753"
    red: "#ef2929"
    green: "#8ae234"
    yellow: "#fce94f"
    blue: "#729fcf"
    magenta: "#ad7fa8"
    cyan: "#34e2e2"
    white: "#eeeeec"

window:
  opacity: 0.4
  padding:
    x: 8
    y: 8

font:
  normal:
    family: Iosevka Nerd Font
    style: Regular
  bold:
    family: Iosevka Nerd Font
    style: Bold
  italic:
    family: Iosevka Nerd Font
    style: Italic
  bold_italic:
    family: Iosevka Nerd Font
    style: Bold Italic
  size: 8.0
  offset:
    x: 0
    y: 0

cursor:
  # Cursor style
  style:
    # Cursor shape
    #
    # Values for `shape`:
    #   - â–‡ Block
    #   - _ Underline
    #   - | Beam
    shape: Block

    blinking: never

    vi_mode_style: on

    blink_interval: 50

    text: '0x1E1D2F'

    cursor: '0xF5E0DC'
theme: Gnometerm
